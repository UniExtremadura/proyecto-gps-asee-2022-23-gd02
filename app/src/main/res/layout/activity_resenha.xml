<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/contenedorResenhas"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ResenhaActivity">

    <TextView
        android:id="@+id/resenhaNombre"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="32dp"
        android:layout_marginTop="16dp"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/resenhaDireccion"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:textSize="18sp"
        app:layout_constraintStart_toStartOf="@+id/resenhaNombre"
        app:layout_constraintTop_toBottomOf="@+id/resenhaNombre" />

    <FrameLayout
        android:id="@+id/contenedorResenhasNuevo"
        android:layout_width="399dp"
        android:layout_height="407dp"
        android:layout_marginTop="4dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/resenhaDireccion">

    </FrameLayout>

</androidx.constraintlayout.widget.ConstraintLayout>